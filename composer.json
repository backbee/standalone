{
    "name": "lp-digital/standalone",
    "description": "BackBee Standalone Demo",
    "keywords": [
        "CMS",
        "Content Management",
        "BackBee",
        "Standalone"
    ],
    "type": "project",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Djoudi Bensid",
            "email": "djoudi.bensid@lp-digital.fr"
        },
        {
            "name": "CÃ©dric Bouillot",
            "email": "cedric.bouillot@lp-digital.fr"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@gitlab.com:lp-digital/backbee/backbee-php.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.com:lp-digital/backbee/core-php.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.com:lp-digital/backbee/doctrine/orm.git"
        },
        {
            "type": "vcs",
            "url": "git@gitlab.com:lp-digital/backbee/toolbar-dist.git"
        }
    ],
    "require": {
        "php": ">=7.1",
        "backbee/backbee-php": "^1.4",
        "backbee/core-php": ">=4.0",
        "doctrine/orm": "v2.4.8.1",
        "backbee/toolbar-dist": ">=4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "~3.2.0"
    },
    "autoload": {
        "psr-4": {
            "BackBeePlanet\\Standalone\\": "src/Standalone",
            "App\\": "src/App"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "BackBee\\Installer\\Composer\\ScriptHandler::moveDistResources",
            "BackBee\\Installer\\Composer\\ScriptHandler::moveConfigDistResources"
        ],
        "post-update-cmd": [
            "BackBee\\Installer\\Composer\\ScriptHandler::moveDistResources",
            "BackBee\\Installer\\Composer\\ScriptHandler::moveConfigDistResources"
        ]
    }
}